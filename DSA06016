#include<bits/stdc++.h>
using namespace std;

int main() {
	int t;
	cin>>t;
	while(t--) {
		int n,m;
		cin>>n>>m;
		int a[n],b[m];
		long long max=-1000000000;
		long long min=1000000000;
		for(int i=0; i<n; i++) {
			cin>>a[i];
			if(a[i]>max)max=a[i];
		}
		for(int i=0; i<m; i++) {
			cin>>b[i];
			if(b[i]<min)min=b[i];
		}
cout<<max*min<<endl;
	}
}
