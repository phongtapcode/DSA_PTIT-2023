//Phong BÃ¡
#include<bits/stdc++.h>
using namespace std;
int x[100],n;

bool check(){
	for(int i=1; i<=n; i++){
		if(x[i]!=x[n+1-i])return false;
	}
	return true;
}
void result() {
	for(int i=1; i<=n; i++) {
		cout<<x[i]<<' ';
	}
	cout<<endl;
}

void Try(int i) {
	for(int j=0; j<=1; j++) {
		x[i]=j;
		if(i==n){
			if(check())result();
		}
		else Try(i+1);
	}
}

int main() {
	cin>>n;
	Try(1);
}
